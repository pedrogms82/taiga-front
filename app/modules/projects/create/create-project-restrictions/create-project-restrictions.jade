div.create-project-warning(ng-if="!canCreatePrivateProjects.valid && canCreatePrivateProjects.reason == 'max_private_projects'")
    tg-svg(svg-icon="icon-exclamation")
    span {{ 'PROJECT.CREATE.MAX_PRIVATE_PROJECTS' | translate }}

div.create-project-warning(ng-if="!canCreatePublicProjects.valid && canCreatePublicProjects.reason == 'max_public_projects'")
    tg-svg(svg-icon="icon-exclamation")
    span {{ 'PROJECT.CREATE.MAX_PUBLIC_PROJECTS' | translate }}